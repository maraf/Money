{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@2.2.4",
      "destination": "wwwroot/lib/jquery/dist"
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@5.3.3",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap.css",
        "dist/css/bootstrap.css.map",
        "dist/css/bootstrap.min.css",
        "dist/css/bootstrap.min.css.map",
        "dist/js/bootstrap.js",
        "dist/js/bootstrap.js.map",
        "dist/js/bootstrap.min.js",
        "dist/js/bootstrap.min.js.map"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "@popperjs/core@2.11.8",
      "destination": "wwwroot/lib/popperjs",
      "files": [
        "dist/umd/popper.js",
        "dist/umd/popper.js.map",
        "dist/umd/popper.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "@fortawesome/fontawesome-free@6.5.2",
      "destination": "wwwroot/lib/fontawesome",
      "files": [
        "css/all.css",
        "css/all.min.css",
        "webfonts/*"
      ]
    }
  ]
}